    crossorigin="anonymous">

  <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,700" rel="stylesheet">


  <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,700" rel="stylesheet">


</head>

<body>
  <nav class="nav-wrapper clearfix">
    <div class="logo">
      <div class="logo-img">
        <img src="./images/logo.png" style="height: 37px; width: 100px">

      </div>

      <button class="btn nav-button" onclick="location.href='browse'" style="margin-left: 20px"> Browse DIY Deals</button>&nbsp;&nbsp;&nbsp;
      <button class="btn nav-button" onclick="location.href='material'"> Sell on Yesari</button>&nbsp;&nbsp;&nbsp;




    </div>
    <div class="nav-menu">

      <ul class="register">

        <li>
          <span class="wish-list fas fa-shopping-cart" onclick="location.href='cart'"></span>
          </a>
        </li>

        <li>
          <button onclick="location.href='signup'">Register</button>
        </li>
        <li>
          <button onclick="location.href='login'">Login</button>
        </li>





      </ul>
      <!-- <div class="register">Register</div>
  <div class="login">Login  </div> -->
    </div>
  </nav>



  <div class="modal-content" style="padding-top: 60px;">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal">&times;</button>
      <center>
        <h2 style="color: #DF6E21;"> LOGIN</h2>
      </center>
    </div>
    <div class="modal-body">
      <form class="form-signin" action="/login" method="POST">
        <input type="text" name="username" class='form-control' placeholder="username" required="" autofocus="">
        <input type="password" name="password" class='form-control' placeholder="password" required="">
        <button class="btn btn-lg btn-primary btn-block" type="submit" style="background: #df6e21; border: #df6e21"> Sign In</button>

        <a href="/signup">Create an account</a>
        <div id="message">
          <% if (message) {%>

            <%=message%>

              <%}%>

        </div>
      </form>
    </div>
    <div class="modal-footer">

      <p>Not a member?
        <a href="/signup"> Register</a>
      </p>
      <p>
        <a href="#">Forgot Password</a>
      </p>
    </div>
  </div>
  </div>
  </div>


