        }

        .first-section .first-block img {
            width: 300px;
            height: 300px;
            opacity: 0.9;
            transition: 200ms;
            transition-timing-function: ease-out;
        }

        .first-section .first-block .content {
            height: 300px;
            width: 300px;
            text-align: right;
            z-index: 2;
            position: absolute;
            transition: 200ms;
            transition-timing-function: ease-out;
            float: left;
            box-shadow: inset 0 0 0 15px #ffffff;


        }

        .first-section .first-block .block-info {
            float: right;
            width: 320px;


        }

        .first-section .first-block .block-title {
            width: 65px;
