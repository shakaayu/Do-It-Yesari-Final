            display: block;
            font-size: 1em;
            text-align: center;
            position: absolute;
            top: 0px;

        }

        .first-section .first-block .content .F {
            display: inline-block;
            font-size: 1.3em;
            font-weight: 900;
            color: black;
            z-index: 2;
        }

        .first-section .first-block .content .block-subtitle {
            /*NEW ARRIVALS*/
            display: none;
            height: 15%;
            width: 95%;
            margin-top: 3%;
            transform: translate3d(10px, 0, 0);
            transition: 200ms;
            transition-timing-function: ease-out;
            letter-spacing: 6px;
            color: blue;
            line-height: 2em;
            font-size: 1.1em;
        }

        .first-section .first-block:hover .block-subtitle {
            display: block;
            transform: translate3d(0px, 0, 0);
        }

        .first-section .first-block:hover .content {
            transform: translate3d(0px, 0, 0);
            box-shadow: none;
        }

        .first-section .first-block:hover img {
            opacity: 0.8;
            filter: sepia(50%);

        }

